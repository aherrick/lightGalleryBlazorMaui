@code {
 
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            try
            {
                await JS.InvokeVoidAsync("initLightGallery");

            }
            catch (Exception ex)
            {
                await JS.InvokeVoidAsync("alert", ex.ToString()); // Alert
            }
        }
    }
}